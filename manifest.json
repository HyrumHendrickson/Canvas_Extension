{
  "manifest_version": 3,
  "name": "Canvas Python Code Editor",
  "version": "2.0",
  "description": "Add a Python code editor with execution capabilities to Canvas LMS. Run Python code directly in your browser with support for NumPy, Pandas, and Matplotlib.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "content_scripts": [
    {
      "matches": ["https://*.instructure.com/*"],
      "js": ["content.js"],
      "css": ["style.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [
    {
      "resources": ["index.html"],
      "matches": ["https://*.instructure.com/*"]
    }
  ],
  "icons": {
    "16": "data:image/svg+xml;base64,",
    "48": "data:image/svg+xml;base64,",
    "128": "data:image/svg+xml;base64,"
  },
  "author": "Hyrum Hendrickson"
}
